            <body>
                <p><!--StartFragment--><!--EndFragment--></p><div><div>#include &lt;stdio.h&gt;</div><div>#include&lt;math.h&gt;</div><div>int main()</div><div>{</div><div>float a,b,c;</div><div>float r1,r2,realpart,imaginarypart,discriminant;</div><div>printf("enter the coefficients a b c ");</div><div>scanf("%f%f%f",&amp;a,&amp;b,&amp;c);</div><div>if(a==0)</div><div>{</div><div>&nbsp; &nbsp; printf("not a quadratic equation");</div><div>&nbsp; &nbsp; return 0;</div><div>}</div><div>discriminant=(b*b) + (4*a*c);</div><div>if(discriminant&gt;0)</div><div>{</div><div>&nbsp; &nbsp; r1=-b+sqrt(discriminant)/(2*a);</div><div>&nbsp; &nbsp; r2=-b-sqrt(discriminant)/(2*a);</div><div>&nbsp; &nbsp; printf("roots are real and different.\n");</div><div>&nbsp; &nbsp; printf("r1=%.2f\n",r1);</div><div>&nbsp; &nbsp; printf("r2=%.2f\n",r2);</div><br><div>}</div><div>else if(discriminant==0)</div><div>{</div><div>&nbsp; &nbsp; r1=-b/2*a;</div><div>&nbsp; &nbsp; printf("the root is real and equal,\n");</div><div>&nbsp; &nbsp; printf("r1=r2=%.2f\n",r1);</div><div>}</div><div>else</div><div>{</div><div>&nbsp; &nbsp; realpart=-b/(2*a);</div><div>&nbsp; &nbsp; imaginarypart=sqrt(-discriminant)/(2*a);</div><div>&nbsp; &nbsp; printf("roots are complex and different.\n");</div><div>&nbsp; &nbsp; printf("r1=%.2f\n",r1);</div><div>&nbsp; &nbsp; printf("r2=%.2f\n",r2);</div><div>}</div><div>return 0;</div><div>}</div><br><br></div>
